		setUpKlaviyoEmailFormFooter('#email_signup', '');
		setUpKlaviyoEmailFormFooter('#email_signup_mobile', '_mobile');
		function setUpKlaviyoEmailFormFooter(formID, fieldName) {
			KlaviyoSubscribe.attachToForms(formID, {
				extra_properties: {
					$source: '$embed',
					$method_type: "Klaviyo Form",
					$method_id: 'embed',
				},
				success: function($form){
					$form.find('#invalid-email-error-message, #invalid-email-error-message-mobile').addClass('d-none');
                    $form.find('#valid-message, #valid-message-mobile').removeClass('d-none').focus();
                    $form.find('#k_id_email, #k_id_email_mobile').attr('aria-describedby', 'valid-message-mobile').hide();
                    $form.find('.k_submit_btn, .k_submit_btn_mobile').attr('aria-describedby', 'valid-message-mobile').hide();
					$form.find('.k_email_input, .k_email_input_mobile').css('border', '0px');
				}
			});
		}

		
		
			
